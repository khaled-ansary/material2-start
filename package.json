{
  "name": "angular4-material2",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve --sourcemaps",
    "serve:aot": "npm run build-themes && ng serve -prod --sourcemaps --aot",
    "build": "ng build",
    "build:aot": "ng build -prod --sourcemaps --aot",
    "lint": "ng lint",
    "workshop": "ts-node ./workshop-cli",
    "build-themes": "bash ./tools/build-themes.sh",
    "bundle-report": "ng build -prod --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "http-server": "npm run build:aot && cd ./dist && angular-http-server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.1",
    "@angular/cdk": "5.0.0-rc0",
    "@angular/common": "^5.0.1",
    "@angular/compiler": "^5.0.1",
    "@angular/core": "^5.0.1",
    "@angular/flex-layout": "2.0.0-beta.10",
    "@angular/forms": "^5.0.1",
    "@angular/http": "^5.0.1",
    "@angular/material": "5.0.0-rc0",
    "@angular/platform-browser": "^5.0.1",
    "@angular/platform-browser-dynamic": "^5.0.1",
    "@angular/platform-server": "^5.0.1",
    "@angular/router": "^5.0.1",
    "@swimlane/ngx-charts": "^6.0.1",
    "angular2gridster": "0.6.9",
    "core-js": "^2.5.1",
    "d3": "^4.10.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.0.1",
    "@types/jasmine": "2.5.46",
    "@types/node": "~7.0.12",
    "angular-http-server": "^1.4.2",
    "codelyzer": "~4.0.1",
    "node-sass": "^4.6.0",
    "simple-git": "^1.69.0",
    "simple-terminal-menu": "^1.1.3",
    "ts-node": "~3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.4.2",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
